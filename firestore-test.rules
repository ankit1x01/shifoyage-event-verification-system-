rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Temporary test mode - any authenticated user can read/write
    match /attendees/{attId} {
      allow read, write: if request.auth != null;
    }

    match /coupons/{code} {
      allow read, write: if request.auth != null;
    }
  }
}